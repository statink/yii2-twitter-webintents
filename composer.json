{
    "name": "statink/yii2-twitter-webintents",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Twitter WebIntents asset bundle for stat.ink",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 7.1.0",
        "yiisoft/yii2": "2.0.*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "statink\\yii2\\twitter\\webintents\\": "src"
        }
    },
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
